<?xml version="1.0" encoding="UTF-8"?>
{% block content %}
    <list-users>
        {% for user in users %}
            <user>
                <id>{{ user.id }}</id>
                <name>{{ user.name }}</name>
                <email>{{ user.email }}</email>
                <password>{{ user.password }}</password>
                <admin>{{ user.admin }}</admin>
                <created-at>{{ user.createdAt }}</created-at>
                <status>{{ user.status }}</status>
            </user>
        {% endfor %}
    </list-users>
{% endblock %}
